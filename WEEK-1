#include<stdio.h>
int main()
{
    int arr[6]={1,12,3,45,32,0},j=0,i=0,num=0;
    printf("enter the number=");
    scanf("%d",&num);
    for(i=0;i<6;i++)
    {
        for(j=1;j<6;j++)
        {
            if((arr[i]+arr[j])==num)
            {
                printf("%d + %d equal to %d\n",arr[i],arr[j],num);
            }
        }
    }
return 0;
}
